    "commit": "cz",
    "prepush": "git diff HEAD --quiet && git fetch && yarn precommit && yarn && yarn test && yarn build",
    "go": "yarn prepush && yarn dev",
    "prepare": "husky install",
    "postinstall": "husky install",
    "codegen": "graphql-codegen",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@chakra-ui/react": "^2.3.1",
